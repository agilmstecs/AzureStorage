@model IEnumerable<storageDemop.Models.ProductsFromTable>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p>
    <h2>
        @Html.ActionLink("Products from cosmos db", "Index", "ProductsFromCosmos")
    </h2>
    <h2>
        @Html.ActionLink("Products from sqldb", "Index", "Products")
    </h2>
    <h2>
        @Html.ActionLink("Blobs", "ListBlob", "Blob")
    </h2>
</p>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PartitionKey)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RowKey)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ETag)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.ProductModel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PartitionKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RowKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ETag)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.ProductModel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
